{
  "name": "wellbank-monorepo",
  "version": "1.0.0",
  "description": "WellBank Healthcare Platform - Monorepo",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "shared"
  ],
  "scripts": {
    "backend:dev": "npm run dev --workspace=backend",
    "backend:build": "npm run build --workspace=backend",
    "backend:start": "npm run start:prod --workspace=backend",
    "frontend:dev": "npm run dev --workspace=frontend",
    "frontend:build": "npm run build --workspace=frontend",
    "shared:build": "npm run build --workspace=shared",
    "install:all": "npm install && npm install --workspaces"
  },
  "keywords": [
    "healthcare",
    "nestjs",
    "nextjs",
    "monorepo"
  ],
  "author": "WellBank Team",
  "license": "PROPRIETARY"
}
